import time
import requests
import threading
# name = "1"

data = {'username':'qqq','password':'qqq','sub':1}

r1 = requests.session()
r2 = requests.session()
url = 'http://127.0.0.1/shopping/login.php'
r1.post(url=url,data=data)
r2.post(url=url,data=data)
buy = 'http://127.0.0.1/shopping/opt.php?name=article6&action=buy'
sale = 'http://127.0.0.1/shopping/opt.php?name=article6&action=sale'


def buys1():
	q2 = r1.post(url=buy)
	print q2.content
	return q2
def buys2():
	q2 = r2.post(url=buy)
	print q2.content
	return q2
def sales1():
	# print 1
	# time.sleep(3)
	q1 = r1.post(url=sale)
	print q1.content
	return q1
def sales2():
	# print 1
	# time.sleep(3)
	q1 = r2.post(url=sale)
	print q1.content
	return q1

for i in range(0,10000):
	threading.Thread(target=buys1, args=()).start()
	threading.Thread(target=buys2, args=()).start()
	time.sleep(2)
	sales1()

